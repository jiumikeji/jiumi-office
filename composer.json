{
    "name": "jiumikeji/jiumi-office",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "九米",
        "九米-office"
    ],
    "description": "河北九米电子科技有限 office package",
    "autoload": {
        "psr-4": {
            "Jiumi\\Office\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JiumiTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "phpoffice/phpspreadsheet": "^1.24"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.tencent.com/composer/"
        }
    }
}
